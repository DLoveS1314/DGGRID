# specify the operation
dggrid_operation GENERATE_GRID

# specify the DGG
dggs_type ISEA4T
dggs_res_spec 3

# control the generation
clip_subset_type WHOLE_EARTH
geodetic_densify 0.0

# specify the outputd

neighbor_output_type TEXT

neighbor_output_file_name outputfiles/trineighbors_3.csv

point_output_type KML
point_output_file_name outputfiles/tri_3_point


densification 0
precision 7